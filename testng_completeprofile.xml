<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	
	<groups>
		<run>
			<include name="competeprofile"/>
		</run>
	</groups>
	
	 <listeners>
	 <listener class-name="org.ecocrewFramework.TestUtils.Listeners"/>
			
 </listeners>
 
  <test thread-count="5" name="Test">
    <classes>
      <class name="org.ecocrewFramework.DiscardProfileTest"/>
    
     
      <class name="org.ecocrewFramework.NewUserLoginCompleteProfileTest"/>    
     
      <class name="org.ecocrewFramework.RaisePickupRequestTest"/>
      <class name="org.ecocrewFramework.EditAddressTest"/>
      <class name="org.ecocrewFramework.RaiseContributionErrorValidationTest"/>
      <class name="org.ecocrewFramework.savedAddressesTest"/>
      <class name="org.ecocrewFramework.ErrorValidationTest"/>  
      <class name="org.ecocrewFramework.DeleteAddressTest"/>
     
      <class name="org.ecocrewFramework.RegisterYourBusinessTest"/>
      <class name="org.ecocrewFramework.AccountSettingTest"/>    
      <class name="org.ecocrewFramework.ExistingUserTest"/>
      <class name="org.ecocrewFramework.NewUserLoginIncompleteProfileTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
